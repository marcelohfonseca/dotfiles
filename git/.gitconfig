[init]
	defaultBranch = main
[pull]
	ff = only
	default = simple
	followTags = true
[commit]
    gpgsign = true
[merge]
    verifySignatures = true
[tag]
    gpgsign = true
[log]
    showSignature = true
[filter "lfs"]
	process = git-lfs filter-process
	required = true
	clean = git-lfs clean -- %f
	smudge = git-lfs smudge -- %f
